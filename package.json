{
  "name": "offender-categorisation",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "compile-sass": "sass --no-source-map --load-path=node_modules/govuk-frontend ./assets/sass/application.sass:./assets/stylesheets/application.css ./assets/sass/application-ie8.sass:./assets/stylesheets/application-ie8.css --style compressed",
    "watch-sass": "npm run compile-sass -- --watch",
    "build": "npm run compile-sass",
    "clean": "rm -rf assets/stylesheets/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 7.11.1"
  },
  "nodemonConfig": {
    "ext": "js,mjs,json,html",
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": 2500
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "agentkeepalive": "^4.2.1",
    "applicationinsights": "^2.3.6",
    "aws-sdk": "^2.1287.0",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.1.3",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "debug": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.3",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^3.14.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.1",
    "joi": "^17.7.0",
    "jwt-decode": "^2.2.0",
    "knex": "^2.4.0",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nocache": "^3.0.4",
    "nunjucks": "^3.2.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.8.0",
    "querystring": "^0.2.1",
    "ramda": "^0.28.0",
    "redis": "^3.1.2",
    "snyk": "^1.1081.0",
    "sqs-consumer": "^5.8.0",
    "superagent": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.5.1",
    "nock": "^13.2.9",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "supertest": "^6.3.3"
  },
  "snyk": true
}
