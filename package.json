{
  "name": "offender-categorisation",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf assets/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">= 14.*",
    "npm": ">= 7.11.1"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": 2500
  },
  "dependencies": {
    "@ministryofjustice/frontend": "0.2.2",
    "agentkeepalive": "^4.1.4",
    "applicationinsights": "^1.8.10",
    "applicationinsights-native-metrics": "0.0.6",
    "aws-sdk": "^2.854.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "bunyan-request-logger": "^2.1.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^4.0.4",
    "cookie-session": "^2.0.0-rc.1",
    "csurf": "^1.11.0",
    "debug": "^4.3.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.1",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^3.11.0",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.3",
    "joi": "^14.3.1",
    "joi-date-extensions": "^1.2.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.95.3",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nocache": "^2.1.0",
    "node-sass": "^4.14.1",
    "nunjucks": "^3.2.3",
    "pac-resolver": "^4.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^7.18.2",
    "querystring": "^0.2.0",
    "ramda": "^0.27.1",
    "redis": "^3.1.1",
    "sass-middleware": "^0.0.3",
    "snyk": "^1.561.0",
    "sqs-consumer": "^5.5.0",
    "superagent": "^5.3.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^7.21.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nock": "^12.0.3",
    "prettier": "^2.2.1",
    "redis-cli": "^1.4.0",
    "supertest": "^4.0.2"
  },
  "snyk": true
}
