{
  "name": "offender-categorisation",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon --inspect server.js | bunyan -o short",
    "start:debug": "node --inspect server.js",
    "compile-sass": "sass --no-source-map --load-path=node_modules/govuk-frontend ./assets/sass/application.sass:./assets/stylesheets/application.css ./assets/sass/application-ie8.sass:./assets/stylesheets/application-ie8.css --style compressed",
    "watch-sass": "npm run compile-sass -- --watch",
    "build": "npm run compile-sass",
    "clean": "rm -rf assets/stylesheets/* .port.tmp *.log dist build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect",
    "copy-views": "cp -R server/views dist/server/",
    "int-test": "export $(cat feature.env) && cypress run --config video=false",
    "int-test-ui": "export $(cat feature.env) && cypress open",
    "watch-node-feature": "export $(cat feature.env) && npm run start:dev",
    "start-feature": "export $(cat feature.env) && node $NODE_DEBUG_OPTION server.js | bunyan -o short",
    "start-feature:dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"Node,Sass\" -c \"yellow.bold,cyan.bold,green.bold,blue.bold\" \"npm run watch-node-feature\" \"npm run watch-sass\""
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 7.11.1"
  },
  "nodemonConfig": {
    "ext": "js,mjs,json,html",
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": 2500
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@snyk/protect": "^1.1206.0",
    "agentkeepalive": "^4.2.1",
    "applicationinsights": "^2.5.1",
    "aws-sdk": "^2.1354.0",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.1.3",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "debug": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.3",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.7.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.1",
    "joi": "^17.7.1",
    "jwt-decode": "^2.2.0",
    "knex": "^2.4.2",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nocache": "^3.0.4",
    "nunjucks": "^3.2.4",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.8.0",
    "querystring": "^0.2.1",
    "ramda": "^0.28.0",
    "redis": "^3.1.2",
    "sqs-consumer": "^5.8.0",
    "superagent": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-format": "^0.2.5",
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "0.0.37",
    "@types/cookie-session": "^2.0.44",
    "@types/csurf": "^1.11.2",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.3",
    "@types/nunjucks": "^3.2.2",
    "@types/passport": "^1.0.12",
    "@types/passport-oauth2": "^1.4.12",
    "@types/superagent": "^4.1.16",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "audit-ci": "^6.6.1",
    "concurrently": "^7.6.0",
    "cookie-session": "^2.0.0",
    "cypress": "^12.17.4",
    "cypress-multi-reporters": "^1.6.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-html-reporter": "^3.7.1",
    "jest-junit": "^15.0.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.1",
    "prettier-plugin-jinja-template": "^0.2.0",
    "sass": "^1.57.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "snyk": true
}
